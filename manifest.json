{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "short_name": "__MSG_extensionShortName__",
  "description": "__MSG_extensionDescription__",
  "author": "Brett Zamir",
  "version": "0.3.0",
  "homepage_url": "https://github.com/brettz9/enumerate/",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "options_ui": {
