    "type": "module",
    "service_worker": "lib/background.js"
  },
  "icons": {
    "128": "icon.png"
  },
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "enumerator@brett-zamir.me"
    }
  }
}
